{
  "name": "@fluencelabs/aqua",
  "version": "0.0.0",
  "description": "Aqua compiler",
  "type": "module",
  "files": [
    "aqua.js",
    "index.js",
    "error.js",
    "utils.js",
    "dist/*",
    "src/*"
  ],
  "bin": {
    "aqua": "index.js",
    "aqua-cli": "error.js"
  },
  "scripts": {
    "run": "node index.js",
    "from:scalajs": "cp ../cli/.js/target/scala-3.0.2/cli-opt/main.js ./aqua.js && npm run run -- $@",
    "build": "tsc"
  },
  "dependencies": {
    "@fluencelabs/fluence": "0.15.2",
    "@fluencelabs/aqua-lib": "0.2.1",
    "ipfs-http-client": "50.1.2",
    "@fluencelabs/aqua-ipfs": "0.5.2"
  },
  "devDependencies": {
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fluencelabs/aqua.git"
  },
  "keywords": [
    "aqua",
    "fluence"
  ],
  "author": "Fluence Labs",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/fluencelabs/aqua/issues"
  },
  "homepage": "https://github.com/fluencelabs/aqua#readme"
}
