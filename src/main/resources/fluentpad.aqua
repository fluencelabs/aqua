data User:
	peer_id: string
	relay_id: string
	name: string

data GetUsersServiceResult:
	users: []User
	ret_code: s32
	err_msg: string

service UserList:
	get_users:  -> GetUsersServiceResult

func getUserList() -> []User:
	on "userlist_node":
		UserList "userlist_id"
		allUsers <- UserList.get_users()
	<- allUsers.users
