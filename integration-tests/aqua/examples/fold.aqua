import "println.aqua"
import "@fluencelabs/aqua-lib/builtin.aqua"

-- showcases `for` instruction that compiles to `fold` in AIR
func iterateAndPrint(strings: []string):
    for s <- strings:
        print(s)

func iterateAndPrintParallel(nodes: []string, c: Info -> ()):
    for s <- nodes par:
        on s:
            ads <- Peer.identify()
            c(ads)

func to_i_64(i: u32) -> i64:
  <- i

func forBug499() -> []i64:
    num = 5
    numbers: *i64
    for i <- [""]:
        ali64 <- to_i_64(num)
        numbers <<- ali64
    <- numbers
