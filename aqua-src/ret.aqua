data DT:
  field: string

service DTGetter("get-dt"):
    get_dt(s: string) -> DT

func use_name1(name: string) -> string, u8:
    results <- DTGetter.get_dt(name)
    <- results.field, 8

func use_name2(name: string) -> []string:
    results: *string
    results <- use_name1(name)
    results <- use_name1(name)
    results <- use_name1(name)
    <- results