service Demo("demo"):
	get4: u64, u64, string -> u64
	bool: -> bool
	log: string, bool -> ()

func initAfterJoin(users: []string) -> []string:
    for user <- users par:
        on user:
            isOnline <- Demo.bool()
            if isOnline:
                on user via "user relay":
                    Demo.log("me", true)
        par Demo.log(user, isOnline)
    <- users